<%= form_with scope: :student , url: students_path, local: true do |form| %>
 
  <% if @student.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@student.errors.count, "error") %> prohibited
        this student form from being saved:
      </h2>
      <ul>
        <% @student.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <p>
    <%= form.label :stuname%><br>
    <%= form.text_field :stuname %>
  </p>
 
  <p>
    <%= form.label :section%><br>
    <%=form.collection_select :section, Student::SECTIONS, :to_s, :to_s, :include_blank => true %> 
   </p> 

 <p>
    <%= form.label :telugu%><br>
    <%= form.text_field :telugu %>
  </p>
  <p>
    <%= form.label :english%><br>
    <%= form.text_field :english %>
  </p>
  <p>
    <%= form.label :hindi%><br>
    <%= form.text_field :hindi %>
  </p>

  
  <p>
    <%= form.submit %>
  </p>
<% end %>
<%= link_to "Back", students_path %>